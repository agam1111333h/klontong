    <style>
      .password-wrapper {
        position: relative;
        display: flex;
        align-items: center;
      }

      .toggle-password {
        position: absolute;
        right: 10px;
        cursor: pointer;
        font-size: x-large;
      }

      .alert {
        padding: 20px;
        background-color: #f44336;
        color: white;
        margin-bottom: 15px;
      }

      .h1 {
        color: white;
      }

      html {
        height: 100%;
      }
      body {
        margin: 0;
        padding: 0;
        font-family: "Poppins", sans-serif;
        background: linear-gradient(#30142b, #2772a1);
      }

      .btn {
        position: relative;
        display: inline-block;
        padding: 10px 20px;
        color: #289bb8;
        font-size: 16px;
        text-decoration: none;
        overflow: hidden;
        transition: 0.5s;
        margin-top: 15px;
        letter-spacing: 2px;
        text-decoration: none;
        border: none;
        cursor: pointer;
      }
      .btn_1 {
        position: relative;
        display: inline-block;
        padding: 10px 20px;
        color: #289bb8;
        font-size: 16px;
        text-decoration: none;
        overflow: hidden;
        transition: 0.5s;
        margin-top: 15px;
        letter-spacing: 2px;
        text-decoration: none;
        border: none;
        cursor: pointer;
      }

      .btn span {
        position: absolute;
        display: block;
      }

      .btn:hover {
        background: #289bb8;
        color: #fff;
        border-radius: 5px;
        box-shadow: 0 0 5px #289bb8, 0 0 25px #289bb8, 0 0 50px #289bb8,
          0 0 100px #289bb8;
      }
    </style>
